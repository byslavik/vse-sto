{
  "presets": [
    "react",
    "es2015"
  ],
  "plugins": [
    "add-module-exports",
    "transform-object-rest-spread",
    "object-values-to-object-keys",
    ["module-resolver", {
      "root": [""],
      "alias": {
        "components": "./src/front/js/components",
        "helpers": "./src/front/js/helpers",
        "pages": "./src/front/js/pages",
        "api": "./src/front/js/api"
      }
    }]
  ]
}